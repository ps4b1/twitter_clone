<%= form_for @chatroom do |f| %>
  <%= f.text_field :room_name, placeholder: "Enter group name" %>
  <%= f.hidden_field :direct, value: false %>
  <%= f.collection_select :user_ids, @users, :id, :username, {}, { multiple: true, class: "form-select w-50%" } %>
  <%= f.button 'Create', class: 'btn  btn-outline-primary' %>
<% end %>